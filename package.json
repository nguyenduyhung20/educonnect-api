{
  "name": "educonnect",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "dev:nodemon": "nodemon --exec npx ts-node --files src/index.ts",
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "eslint src/ --ext .ts --fix",
    "format": "prettier src/ --write",
    "precommit": "npm run format && npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "kysely-codegen": "kysely-codegen --out-file src/interfaces/db.d.ts",
    "prisma:pull": "prisma db pull",
    "prisma:gen": "prisma generate",
    "prisma:update": "npm run prisma:pull && npm run prisma:gen",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint  --quiet --fix",
      "prettier  --write --ignore-unknown"
    ],
    "*.{json,html}": [
      "prettier  --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express-fileupload": "^1.4.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "cookie-parser": "^1.4.6",
    "@types/kafkajs": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "kysely": "^0.26.3",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.7",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "prisma-kysely": "^1.7.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  }
}
