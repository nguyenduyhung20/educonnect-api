{
  "name": "educonnect",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec npx ts-node --files src/index.ts",
    "def": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc",
    "lint": "eslint src/ --ext .ts --fix",
    "format": "prettier src/ --write",
    "precommit": "npm run format && npm run lint && npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "kysely-codegen": "kysely-codegen --out-file src/interfaces/db.d.ts",
    "prisma:pull": "prisma db pull",
    "prisma:gen": "prisma generate",
    "prisma:update": "npm run prisma:pull && npm run prisma:gen",
    "prepare": "husky  install",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint  --quiet --fix",
      "prettier  --write --ignore-unknown"
    ],
    "*.{json,html}": [
      "prettier  --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "kysely": "^0.27.3",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.5",
    "prisma-kysely": "^1.8.0",
    "redis": "^4.6.13",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/kafkajs": "^1.9.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.6",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4"
  },
  "optionalDependencies": {
    "husky": "^9.0.11"
  }
}
